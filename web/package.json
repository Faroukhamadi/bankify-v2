{
	"name": "web",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --open",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync && npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"generate": "graphql-codegen --config codegen.yml",
		"gen": "graphql-codegen --config ./.graphqlrc.yaml",
		"cypress:open": "cypress open"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.6.2",
		"@graphql-codegen/introspection": "^2.1.1",
		"@graphql-codegen/named-operations-object": "^2.2.1",
		"@graphql-codegen/typescript": "^2.5.1",
		"@graphql-codegen/typescript-operations": "^2.4.2",
		"@graphql-codegen/typescript-urql": "^3.5.12",
		"@graphql-codegen/urql-introspection": "^2.1.1",
		"@graphql-codegen/urql-svelte-operations-store": "^1.1.20",
		"@material/typography": "^13.0.0",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/circular-progress": "^6.0.0-beta.16",
		"@smui/data-table": "^6.0.0-beta.16",
		"@smui/drawer": "^6.0.0-beta.16",
		"@smui/fab": "^6.0.0-beta.16",
		"@smui/icon-button": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/paper": "^6.0.0-beta.16",
		"@smui/tab": "^6.0.0-beta.16",
		"@smui/tab-bar": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "^1.0.0-next.53",
		"@sveltejs/kit": "next",
		"@types/jest": "^28.1.4",
		"@types/node": "^18.0.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"cypress": "^10.3.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2"
	},
	"type": "module",
	"dependencies": {
		"@apollo/client": "^3.5.6",
		"@kitql/all-in": "^0.6.11",
		"@urql/svelte": "^2.0.2",
		"graphql": "^16.5.0",
		"svelte-apollo": "^0.5.0",
		"urql": "^2.2.2"
	}
}
